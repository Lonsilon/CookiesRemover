{

  "manifest_version": 2,
  "name": "Cookies Remover",
  "version": "1.0",
  "icons": {
    "96": "icons/icon_96.png"
  },

  "description": "Eliminador de cookies.",

  "permissions": ["activeTab"],

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": {
      "16": "icons/icon_16.png",
      "32": "icons/icon_32.png"
    }
  }

}